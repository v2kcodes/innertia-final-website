{
  "name": "innertia-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --local > lib/database.types.ts",
    "db:migrate": "supabase migration new",
    "db:push": "supabase db push",
    "deploy:preview": "vercel",
    "deploy:prod": "vercel --prod",
    "env:pull": "vercel env pull .env.local",
    "env:list": "vercel env ls",
    "ui:add": "npx shadcn-ui@latest add",
    "analyze": "ANALYZE=true next build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:cross-browser": "playwright test tests/cross-browser-compatibility.spec.ts",
    "test:performance": "playwright test tests/performance-audit.spec.ts --project=performance-chrome",
    "test:accessibility": "playwright test tests/cross-browser-compatibility.spec.ts --project=accessibility-chrome",
    "test:mobile": "playwright test --project=mobile-*",
    "test:desktop": "playwright test --project=*-desktop-*",
    "test:all-browsers": "playwright test --project=chromium-* --project=firefox-* --project=webkit-* --project=edge-*",
    "test:comprehensive": "playwright test --reporter=html",
    "lighthouse:audit": "lighthouse http://localhost:3000 --output=html --output-path=test-results/lighthouse-report.html",
    "axe:audit": "axe http://localhost:3000 --save test-results/axe-report.json",
    "audit:full": "npm run lighthouse:audit && npm run axe:audit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^10.12.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.5.4",
    "lucide-react": "^0.441.0",
    "next": "^14.2.13",
    "next-themes": "^0.4.6",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.10.2",
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.47.2",
    "axe-playwright": "^2.2.2",
    "canvas": "^3.2.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.13",
    "lighthouse": "^12.8.2",
    "prettier": "^3.3.3",
    "sharp": "^0.34.3"
  }
}
